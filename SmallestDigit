#include <stdio.h>
#include<limits.h>
int DigitMin(int No){
  int minDigit = INT_MAX;
  while(No > 0){
    int digit = No % 10;
    if(digit < minDigit){
      minDigit = digit;
    }
    No /= 10;
  }
  return minDigit;
}
int main(){
  int val=0;
  int res=0;
  printf("Enter the no.");
  scanf("%d", &val);
  res = DigitMin(val);
  printf("Smallest digit is %d", res);
  return 0;
}
