#include <stdio.h>
#define TRUE 1
#define FALSE 0
typedef int BOOL;

BOOL chkAnagram(int iNo1, int iNo2){
  int count1[10] = {0};
  int count2[10] = {0};

  while(iNo1 > 0){
    int digit = iNo1 % 10;
    count1[digit]++;
    iNo1 = iNo1 / 10;
  }

  while(iNo2 > 0){
    int digit = iNo2 % 10;
    count2[digit]++;
    iNo2 = iNo2 / 10;
  }
//compare
  for(int i=0 ; i<10 ; i++){
    if(count1[i] != count2[i]){
      return FALSE;
    }
  }
  return TRUE;
}

int main(){
  int val1=0, val2=0;
  BOOL res = FALSE;
  printf("Enter the 1st no.");
  scanf("%d", &val1);
  printf("Enter the 2nd no.");
  scanf("%d", &val2);
  res = chkAnagram(val1, val2);
  if(res == TRUE){
    printf("No. is Anagram");
  }
  else{
    printf("Not Anagram");
  }
}
