#include <stdio.h>

void displayNum(int No1 , int No2){
  int min = (No1 < No2) ? No1 : No2;
  printf("Common factor is :");
  for(int i=1 ; i<=min ; i++){
    if(No1%i == 0 && No2%i == 0){
      printf("%d ",i);
    }
  }
  printf("\n ");
}

int main(){
  int val1=0,val2=0;
  printf("Enter 2 no");
  scanf("%d %d", &val1, &val2);
  displayNum(val1,val2);
  return 0;
}
